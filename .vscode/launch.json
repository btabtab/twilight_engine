  "version": "0.2.0",
  "configurations": [
      {
          "name": "DEBUG_CELL_CLANG",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceRoot}/twilightEngineFrameWork.debug.runme",
          "args": ["twilightEngineBuildTest"],
          "stopAtEntry": false,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "miDebuggerPath": "/usr/bin/gdb",
          "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            }
          ],
          "preLaunchTask": "BUILD_WITH_CLANG"
        }
  ]
}